/* pages/persona/manage/manage.wxss */
@import "../../template/star-template/star-template.wxss";

.manage {
  height: 60rpx;
  display: flex;
  width: 100%;
  justify-content: space-between;
  margin-top: 20rpx;
  margin-bottom: 70rpx;
}

.manage view {
  height: 60rpx;
  line-height: 60rpx;
  width: 50%;
  border: 1px solid #999;
  text-align: center;
}

.block {
  margin-top: 30rpx;
}

.new view {
  height: 60rpx;
  width: 600rpx;
  line-height: 60rpx;
  border: 1px solid #999;
  margin: 0 auto;
  text-align: center;
  margin-top: 50rpx;
  margin-bottom: 30rpx;
}
